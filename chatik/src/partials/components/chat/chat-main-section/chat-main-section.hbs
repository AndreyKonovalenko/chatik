<div class="chat-main-section">
    {{> components/chat/search-bar/search-bar }}
    {{#> components/chat/chat-list/chat-list header=uiConstants.headers.CHAT_LIST controls=true }}
      {{#each _mock_chats}}
        {{> components/chat/chat-card/chat-card title=this.title avatar=this.avatar content=this.last_message.content time=this.last_message.time}}
      {{/each}}
 
    {{/ components/chat/chat-list/chat-list}}
</div>